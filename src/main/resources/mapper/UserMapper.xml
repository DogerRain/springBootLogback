<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yudianxx.springBootDemo.mapper.UserMapper" >
    <select id="selectByPrimaryKey" resultType="com.yudianxx.springBootDemo.model.User" parameterType="java.lang.Integer" >
      SELECT userid as id,realName as userName  FROM cg_user where  userId = #{userId}
    </select>
</mapper>